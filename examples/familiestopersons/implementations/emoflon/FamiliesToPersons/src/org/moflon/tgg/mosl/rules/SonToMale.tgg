#using FamiliesToPersons.*
#using AttrCondDefLibrary.*

#rule SonToMale #extends FamilyMember2Person #with FamiliesToPersons

#source { 
	++ f : Family {
		++ -sons->fm	
	}
	
	++ fm : FamilyMember
}

#target {
	++ p : Male
}

#correspondence {
	++ familyMember2Persons : FamilyMemberToPerson {
		#src->fm
		#trg->p
	}
}

#attributeConditions {
	
}
