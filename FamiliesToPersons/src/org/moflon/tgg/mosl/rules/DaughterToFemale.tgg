#using FamiliesToPersons.*
#using AttrCondDefLibrary.*

#rule DaughterToFemale #extends FamilyMember2Person #with FamiliesToPersons

#source { 
	++ f : Family {
		
	}
	++ fm : FamilyMember {
		++ -family_daughter->f
	} 
}

#target {
	++ p : Female
}

#correspondence {
	++ familyMember2Persons : FamilyMemberToPerson {
		#src->fm
		#trg->p
	}
}

#attributeConditions {
	
}
